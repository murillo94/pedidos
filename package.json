{
  "name": "pedidos",
  "version": "1.0.0",
  "description": "Uma aplicação que adiciona pedidos e ​altera os pedidos existentes.",
  "author": "Murillo de Miranda Pereira",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.9.0"
  },
  "dependencies": {
    "add": "^2.0.6",
    "dinero.js": "^1.5.0",
    "esm": "^3.2.5",
    "firebase": "^5.8.2",
    "formik": "^1.5.0",
    "next": "^8.0.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-focus-lock": "^1.17.7",
    "react-number-format": "^4.0.6",
    "react-select": "^2.3.0",
    "validator": "^10.11.0",
    "yarn": "^1.13.0",
    "yup": "^0.26.10"
  },
  "scripts": {
    "dev": "next ./src",
    "now-build": "next build ./src && next export ./src -o dist",
    "deploy": "now && now alias",
    "test": "jest",
    "lint": "eslint .",
    "init-firebase": "node -r dotenv/config -r esm src/scripts/firebase.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "next/babel"
    ],
    "plugins": [
      "babel-plugin-jsx-remove-data-test-id"
    ]
  }
}
